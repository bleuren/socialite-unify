{
    "name": "bleuren/socialite-unify",
    "description": "A Laravel package for LINE social authentication with Jetstream integration",
    "type": "library", 
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bleuren\\SocialiteUnify\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.3",
        "laravel/socialite": "^5.18",
        "livewire/livewire": "^3.0",
        "socialiteproviders/line": "^4.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bleuren\\SocialiteUnify\\Providers\\SocialiteServiceProvider"
            ]
        }
    }
}
